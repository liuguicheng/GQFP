<script type="text/javascript">
        var x = -155;
		var y = -95;
        var myTitle = ""
        function yidao(e, obj) {

            myTitle = obj.title;
            obj.title = "";
            var tooltip = "<div id='tooltip'style='width:350px;height:80px;;text-align:center' >" + myTitle + "</div>";   //创建DIV元素
            $("#divMapTree").append(tooltip);  //追加到文档中
            $("#tooltip").css({ "top": (e.pageY + y) + "px", "left": (e.pageX + x) + "px" }).show(); //设置X  Y坐标， 并且显示
        }
        function yidong(e) {
            $("#tooltip").css({ "top": (e.pageY + y) + "px", "left": (e.pageX + x) + "px" });
        }
        function yichu(obj) {
            obj.title = myTitle;
            $("#tooltip").remove();    //移除
        }
    
</script>
